package com.tencent.msdk.stat;

import com.tencent.msdk.tools.Logger;

/**
 * @author hardyshi 2014-03-11
 * 公告类型宏
 */
public enum eEVENT_TYPE {
	eEVENT_NOTICE_SHOW(1),  //展示公告
	eEVENT_NOTICE_MORE(2),	//点击公告详情
	
	eEVENT_BASIC_MD5BAD(10);	//公告图片MD5校验失败
	
    int value = 0;
    eEVENT_TYPE(int val) {
        this.value = val;
    }
    
    static public eEVENT_TYPE getEnum(int i) {
        eEVENT_TYPE type = null;
        switch (i) {
            case 1:
                type = eEVENT_TYPE.eEVENT_NOTICE_SHOW;
                break;
            case 2:
                type = eEVENT_TYPE.eEVENT_NOTICE_MORE;
                break;
            case 10:
                type = eEVENT_TYPE.eEVENT_BASIC_MD5BAD;
                break;
            default:
            	Logger.e("bad event type:"+i);
            	type = eEVENT_TYPE.eEVENT_NOTICE_SHOW;
                break;
        }
        return type;
    }
    
    public int val() {
    	return value;
    }
    
}
